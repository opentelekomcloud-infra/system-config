GITEA_IMAGE={{ gitea_image }}
APP_NAME={{ gitea_app_name }}
ROOT_URL={{ gitea_root_url }}
DB_TYPE=postgres
DB_HOST={{ gitea_db_host }}
DB_NAME={{ gitea_db_name }}
DB_USER={{ gitea_db_username }}
DB_PASSWORD={{ gitea_db_password }}
DISABLE_REGISTRATION=true
SECRET_KEY={{ gitea_secret_key }}
GITEA_SERVER_CERT_FILE=/etc/ssl/gitea/{{ gitea_cert }}-fullchain.crt
GITEA_SERVER_KEY_FILE=/etc/ssl/gitea/{{ gitea_cert }}.pem
